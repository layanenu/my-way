{
  "expo": {
    "name": "MyWay",
    "slug": "MyWay",
    "version": "1.0.0",
    "orientation": "default",
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "default"
        }
      ]
    ],
    "scheme": "myapp",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ios.myWay"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.android.myWay"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}